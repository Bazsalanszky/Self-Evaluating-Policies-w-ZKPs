{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Prolog Files",
            "type": "prolog",
            "request": "launch",
            "runtimeExecutable": "C:/Program files/swipl/bin/swipl",
            "runtimeArgs": [],
            "env": {},
            "program": ".\\gas-support-policy-prolog\\policy.pl",
            "cwd": "${workspaceRoot}",
            "startupQuery": "start",
            "stopOnEntry": false,
            "internalConsoleOptions": "neverOpen",
            "traceCmds": {
                "continue": [
                    "leap",
                    "l"
                ],
                "stepover": [
                    "skip",
                    "s"
                ],
                "stepinto": [
                    "creep",
                    "c"
                ],
                "stepout": [
                    "up",
                    "u"
                ]
            }
        }
    ]
}